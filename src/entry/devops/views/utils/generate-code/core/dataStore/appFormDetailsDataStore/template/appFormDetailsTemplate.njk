<# 递归渲染组件：固定组件名称渲染，不做动态处理。 #><% macro recursiveComponent(model, isSub) %><% if model.type == 'ElDescriptions' %><% if isSub == true %><ElDescriptionsItem <% if model.properties.subSpan %>span="<$ model.properties.subSpan $>"<% endif %>><% endif %><ElDescriptions <$ model.attrs $>><% if model.properties.useSlot == true %>    <template #title><$ model.properties.title $></template><% endif %><% if model.properties.extra %><template #extra><$ model.properties.extra $></template><% endif %><% if model.children %><% for sub in model.children %>    <$ recursiveComponent(sub, true) $><% endfor %><% endif %></ElDescriptions><% if isSub == true %></ElDescriptionsItem><% endif %><% endif %><% if model.type == 'ElDescriptionsItem' %>    <ElDescriptionsItem <% if model.properties.isText == false and model.properties.useSlot == false and model.properties.wrapInline == false %>label="<$ model.properties.label $>"<% endif %> <$ model.attrs $>>        <% if model.properties.wrapInline == true %><div>            <span class="color-999"><$ model.properties.label $></span>        <% endif %>        <% if model.properties.useSlot == true and model.properties.wrapInline == false %><template #label><$ model.properties.label $></template><% endif %>        <% if model.properties.props %>{{ formData.<$ model.properties.props $> }}<% else %><% if model.properties.text %><$ model.properties.text $><% endif %><% endif %><% if model.children %><% for sub in model.children %><$ recursiveComponent(sub, true) $><% endfor %><% endif %>        <% if model.properties.wrapInline == true %></div><% endif %></ElDescriptionsItem><% endif %><% if model.type == 'Operations' %><ElDescriptionsItem rowClassName="description__operations-item">    <div class="description__operations description__operations--<$ model.properties.align $>">        <% for c in model.properties.count %><div class="operations__btn"><$ c $></div><% endfor %>    </div></ElDescriptionsItem><% endif %><% if model.type === 'StatusTag' %><ElDescriptionsItem rowClassName="description__status-tag__container">    <% for c in model.properties.list %><div class="description__status-tag__item <$ c.className $><% if c.classInfo %> <$ c.classInfo.className $><% endif %>"><$ c.label $></div><% endfor %></ElDescriptionsItem><% endif %><% if model.type === 'Image' %><ElDescriptionsItem rowClassName="description__image__container" contentClassName="description__image__content">    <div class="description__image__wrapper">        <div class="description__image__label"><$ model.properties.label $></div>        <div class="description__image__body"><% if model.properties.prop %>            <div class="image__wrapper">               <img :src="formData.<$ model.properties.prop $>" alt="图片" />           </div><% endif %><% for c in model.properties.srcListStr %>           <div class="image__wrapper">               <img src="<$ c $>" alt="图片" />           </div><% endfor %>        </div>    </div></ElDescriptionsItem><% endif %><% endmacro %><script lang="ts">import { Vue, Component } from 'vue-property-decorator'import ElDescriptionsItem from '@/sharegood-ui/packages/base-form-details/components/el-descriptions-item/index'import ElDescriptions from '@/sharegood-ui/packages/base-form-details/components/el-descriptions/index'@Component({    name: '<$ appConfig.name $>',    components: {        ElDescriptions,        ElDescriptionsItem,    },})export default class <$ appConfig.name $> extends Vue {    public formData = <$ formData $>    public created(): void {}    public mounted(): void {}}</script><template>    <div class="app-form-details__container reset-app-form-details__container app-descriptions__container">        <div class="app-form-details__content reset-app-form-details__content"><% for model in columns%>            <$ recursiveComponent(model, false) $><% endfor %>        </div>        <% if config.formConfig.footerButton %>        <div class="app-form-details__footer reset-app-form-details__footer">            <div class="btn">提交</div>        </div>        <% endif %>    </div></template><style scoped lang="less">@import '/src/sharegood-ui/packages/base-form-details/assets/app-descriptions.less';@import '~@/entry/<$ appConfig.cacheConfig.applicationName $>/styles/reset/app-form-details-reset.less';<% if formConfig.statusTagInfo.length > 0 %>.app-form-details {    &__content {        .description__status-tag {            &__container {                <% for item in formConfig.statusTagInfo %>                .<$ item.className $> {                    <$ item.classContent $>                }                <% endfor %>            }        }    }}<% endif %></style>