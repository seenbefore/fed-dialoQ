{
    "name": "vue-template-admin",
    "version": "1.0.0",
    "private": true,
    "description": "基于 vue-cli4 + TypeScript 搭建后台管理模板",
    "bin": {},
    "scripts": {
        "dev": "vue-cli-service serve --open",
        "build": "vue-cli-service build --mode=production",
        "build:fat": "vue-cli-service build --mode=fat",
        "build:uat": "vue-cli-service build --mode=uat",
        "build:prd": "vue-cli-service build --mode=production",
        "lint": "stylelint **/*.{css,less,html,vue} --fix && vue-cli-service lint",
        "new": "plop admin",
        "spa": "plop spa",
        "api": "node api-generator.js",
        "fix": "npx eslint --fix --ext .js,.ts,.vue src",
        "eslint": "npx eslint --ext .js,.ts,.vue src",
        "tinypng": "cross-env ENTRY=src/entry node build/tinypng",
        "report": "npm run build -- --report",
        "report:devops": "npm run build:fat1:devops -- --report",
        "commit": "git cz",
        "generate:entry": "node core/developmentTools/generateTemplate/index.js",
        "update:core": "node core/developmentTools/updateCore/index.js"
    },
